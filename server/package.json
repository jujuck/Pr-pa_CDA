{
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "prod": "ts-node-dev ./src/database/seed.ts && ts-node-dev src/index.ts",
    "grep": "cd ./src/database && gh repo list jujuck --json name,url,owner,languages,isPrivate,id >> raw.json",
    "create": "cd ./src/database && node create.js",
    "seed": "ts-node-dev ./src/database/seed.ts",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write '*/**/*.{js,jsx,ts,tsx}'"
  },
  "lint-staged": {
    "*.sql": "prettier --check",
    "*.ts": "eslint --report-unused-disable-directives"
  }
}
